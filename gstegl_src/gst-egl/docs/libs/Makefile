	$(top_builddir)/gst-libs/gst/uridownloader/libgsturidownloader-1.0.la \
	$(top_builddir)/gst-libs/gst/mpegts/libgstmpegts-1.0.la \
  $(GST_BASE_LIBS)

GTKDOC_CC = $(LIBTOOL) --tag=CC --mode=compile $(CC)
GTKDOC_LD = $(LIBTOOL) --tag=CC --mode=link $(CC)

